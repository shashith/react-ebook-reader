    "version": "0.1.0",
    "private": false,
    "license": "(ISC OR GPL-3.0)",
    "main": "dist/index.js",
    "module": "dist/index.js",
    "babel": {
        "presets": [
            "@babel/react"
        ]
    },
    "dependencies": {
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "react-scripts": "3.4.4"
    },
    "scripts": {
